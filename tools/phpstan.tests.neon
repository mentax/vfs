includes:
	- phpstan.base.neon

parameters:
	level: 6
	resultCachePath: %currentWorkingDirectory%/var/tools/PHPStan/resultCache.tests.php

	ignoreErrors:
		# It has, we are testing access time
		- message: '#^Call to function file_get_contents\(\) on a separate line has no effect\.$#'
		  path: ../tests/Unit/StreamWrapperTest.php
		  count: 1
